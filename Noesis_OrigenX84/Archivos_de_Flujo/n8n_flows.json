{
  "nodes": [
    {
      "id": "1",
      "name": "Nexus Prime Webhook",
      "type": "n8n-nodes-base.webhook",
      "parameters": {
        "path": "sync_node",
        "method": "POST",
        "responseMode": "onReceived",
        "options": {}
      },
      "position": [200, 300]
    },
    {
      "id": "2",
      "name": "Process Data",
      "type": "n8n-nodes-base.function",
      "parameters": {
        "functionCode": "return [{ json: { message: 'Node Synced', status: 'Success' } }];"
      },
      "position": [400, 300]
    },
    {
      "id": "3",
      "name": "LLaMA Processor",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "http://localhost:8502/llama/process",
        "method": "POST",
        "responseFormat": "json",
        "options": {}
      },
      "position": [600, 300]
    },
    {
      "id": "4",
      "name": "Save to Database",
      "type": "n8n-nodes-base.mysql",
      "parameters": {
        "operation": "insert",
        "table": "nexus_prime_data",
        "fields": {
          "fieldNames": ["node_id", "status"],
          "fieldValues": ["={{$json.node_id}}", "={{$json.status}}"]
        }
      },
      "position": [800, 300]
    }
  ],
  "connections": {
    "Nexus Prime Webhook": {
      "main": [
        [{ "node": "Process Data", "type": "main", "index": 0 }]
      ]
    },
    "Process Data": {
      "main": [
        [{ "node": "LLaMA Processor", "type": "main", "index": 0 }]
      ]
    },
    "LLaMA Processor": {
      "main": [
        [{ "node": "Save to Database", "type": "main", "index": 0 }]
      ]
    }
  }
}

